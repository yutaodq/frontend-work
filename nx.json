{
  "npmScope": "frontend-work",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "ivds": {
      "tags": ["scope:ivds","type:app"]
    },
    "ivds-e2e": {
      "tags": ["scope:ivds", "type:e2e"],
      "implicitDependencies": ["ivds"]
    },
    "shared-ui": {
      "tags": ["scope:shared", "type:ui"]
    }
  }
}
